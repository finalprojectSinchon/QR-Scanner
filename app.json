{
  "expo": {
    "name": "qr-auth-app",
    "slug": "qr-auth-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/skyCareLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/skyCareLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yoonjae.qrauthapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1159fba5-598d-479e-9727-8e7a3aced89e"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}